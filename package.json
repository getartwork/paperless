{
  "name": "paperless-reg",
  "version": "0.0.1",
  "description": "Event registration for metadata-paperless",
  "author": "Evgeniy Malyarov <info@oknosoft.ru> (http://www.oknosoft.ru)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "^6.0",
    "npm": "^3.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint src tests server",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "dev:no-debug": "npm run dev -- --no_debug",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "babel-node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon --exec babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "babel-node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oknosoft/paperless"
  },
  "dependencies": {
    "babel-cli": "^6.14",
    "babel-core": "^6.14",
    "babel-loader": "^6.2",
    "babel-plugin-transform-async-to-generator": "^6.8",
    "babel-plugin-transform-runtime": "^6.15",
    "babel-polyfill": "^6.13",
    "babel-preset-es2015": "^6.16",
    "babel-preset-react": "^6.16",
    "babel-preset-stage-0": "^6.5",
    "babel-register": "^6.14",
    "babel-runtime": "^6.11",    
    "classnames": "^2.2",
    "css-loader": "^0.25",
    "cssnano": "^3.8",
    "debug": "^2.2",
    "extract-text-webpack-plugin": "^1.0",
    "file-loader": "^0.9",
    "fs-extra": "^0.30",
    "history": "^2.1",
    "html-webpack-plugin": "^2.24",
    "imports-loader": "^0.6",
    "ip": "^1.1.2",
    "json-loader": "^0.5",
    "metadata-core": "2.0.0-beta.8",
    "metadata-pouchdb": "2.0.0-beta.8",
    "metadata-redux": "2.0.0-beta.8",
    "metadata-react-ui": "2.0.0-beta.8",
    "node-sass": "^3.9",
    "normalize.css": "^5.0",
    "postcss-loader": "^1.0",
    "react": "^15.3",
    "react-addons-shallow-compare": "^15.3",
    "react-dom": "^15.3",
    "react-redux": "^4.0",
    "react-router": "^2.7",
    "react-router-redux": "^4.0",
    "redux": "^3.0",
    "redux-thunk": "^2.0",
    "rimraf": "^2.5.1",
    "sass-loader": "^4.0",
    "style-loader": "^0.13",
    "superlogin-client": "^0.4",
    "url-loader": "^0.5",
    "webpack": "^1.13",
    "yargs": "^4.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.0",
	"better-npm-run": "^0.0.11",
    "chai": "^3.5",
    "chai-as-promised": "^5.3",
    "chai-enzyme": "^0.5",
    "cheerio": "^0.20",
    "codecov": "^1.0",
    "enzyme": "^2.5",
    "eslint": "^3.4",
    "eslint-config-standard": "^6.0",
    "eslint-config-standard-react": "^4.0",
    "eslint-plugin-babel": "^3.3",
    "eslint-plugin-promise": "^3.3",
    "eslint-plugin-react": "^6.4",
    "eslint-plugin-standard": "^2.0",
    "isparta-loader": "^2.0",
    "karma": "^1.2",
    "karma-coverage": "^1.1",
    "karma-mocha": "^1.1",
    "karma-mocha-reporter": "^2.1",
    "karma-phantomjs-launcher": "^1.0",
    "karma-webpack-with-fast-source-maps": "^1.10",
    "koa": "^2.0",
    "koa-connect-history-api-fallback": "^0.3",
    "koa-convert": "^1.2",
    "koa-proxy": "^0.6",
    "koa-static": "^3.0",
    "mocha": "^3.0",
    "nodemon": "^1.10",
    "phantomjs-prebuilt": "^2.1",
